<h1>Sign in</h1>

<%= form_for @conn, Routes.session_path(@conn, :new), [as: :user], fn f -> %>

  <%= label f, :email %>
  <%= text_input f, :email %>
  <%= error_tag f, :email %>

  <%= label f, :password %>
  <%= text_input f, :password %>
  <%= error_tag f, :password %>

  <div>
    <%= submit "Sign in" %>
  </div>
<% end %>
